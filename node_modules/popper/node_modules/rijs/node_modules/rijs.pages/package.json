{
  "name": "rijs.pages",
  "version": "1.0.0",
  "main": "dist",
  "browser": "./node_modules/utilise/identity.js",
  "author": {
    "name": "Pedram Emrouznejad",
    "url": "https://github.com/pemrouz"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rijs/pages.git"
  },
  "scripts": {
    "ignore": "find ./dist -type f -exec sed -i \"s/function _interopRequire/\\/* istanbul ignore next *\\/\\nfunction _interopRequire/g\" {} ;",
    "babel": "babel src -d dist",
    "copy": "cp ./node_modules/rijs/dist/ripple* ./dist",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run babel && npm run ignore && npm run copy",
    "test": "istanbul test ./node_modules/mocha/bin/_mocha --report html -- -R spec",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && rm -rf ./coverage",
    "cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags"
  },
  "license": "pemrouz.mit-license.org",
  "devDependencies": {
    "babel-cli": "*",
    "babel-preset-es2015": "*",
    "browserify": "*",
    "chai": "*",
    "popper": "*",
    "coveralls": "*",
    "istanbul": "*",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "uglify-js": "*",
    "rijs": "*",
    "supertest": "^1.0.1"
  },
  "dependencies": {
    "utilise": "*"
  },
